{
  "name": "frontier-lite",
  "version": "0.0.0",
  "description": "Frontier Lite Electron Application",
  "author": "LBL",
  "main": "dist-electron/main.js",
  "scripts": {
    "ng": "ng",
    "build:electron": "tsc -p tsconfig.electron.json && cpx \"queries/**/*\" dist-electron/queries",
    "start": "npm run build:electron && electron-forge start",
    "electron": "ng build && npm run build:electron && electron .",
    "electron:dev": "cpx \"queries/**/*\" dist-electron/queries && concurrently \"ng build --watch --configuration development\" \"tsc -p tsconfig.electron.json --watch\" \"chokidar 'queries/**/*.sql' -c 'cp -r queries dist-electron/'\" \"electron .\"",
    "electron:prod": "ng build --configuration production && npm run build:electron && electron .",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "package": "npm run build:electron && electron-forge package",
    "make": "npm run build:electron && electron-forge make"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.15",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.15",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@ngrx/signals": "^19.2.1",
    "better-sqlite3": "^12.2.0",
    "chart.js": "^4.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "maplibre-gl": "^5.6.0",
    "rxjs": "~7.8.0",
    "terra-draw": "^1.8.0",
    "terra-draw-maplibre-gl-adapter": "^1.1.1",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.11",
    "@angular/cli": "^19.2.11",
    "@angular/compiler-cli": "^19.2.0",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.0.10",
    "@watergis/maplibre-gl-terradraw": "^1.3.14",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "cpx": "^1.5.0",
    "electron": "^36.2.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2"
  }
}
